                                   new SelectListItem {Text="Finished Good",Value="Finished Good" },
                                   new SelectListItem {Text="Description",Value="Description" },
                                   new SelectListItem {Text="Customer Part",Value="Customer Part" },
                                   new SelectListItem {Text="Customer ID",Value="Customer ID" },
                                   new SelectListItem {Text="Customer Name",Value="Customer Name" },
                                    },  new { @id = "ddlFinishGoodsItem_FIExp2", @class = "form-control" })
                    </div>
                    <div class="col-lg-3">
                        <input type="text" id="txtFinishGoodsItem_FIExp" class="form-control" />
                        @*<input type="hidden" id="hdnFinishGoodsItem" />*@
                    </div>

                </div>


                <table class="table" id="tblFinishGoodsItem_FIExp">
                    <br />
                    <thead>


                        <tr>
                            <th></th>
                            <th>
                                Description
                            </th>


                            <th>
                                FinishedGood
                            </th>
                            <th>
                                CustomerPart
                            </th>
                            <th>
                                CustomerID
                            </th>
                            <th>
                                CustomerName
                            </th>

                        </tr>
                    </thead>
                    <tbody>
                        @foreach (BO_FilterOption teacher in Model.FinishGoodItem)
                        {
                            <tr>
                                <td>
                                    <input type="radio" name="rdBtnFinishGoodsItem_FIExp" id="rdBtnFinishGoodsItem_FIExp" />
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => teacher.Description)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => teacher.FinishedGood)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => teacher.CustomerPart)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => teacher.CustomerID)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => teacher.CustomerName)
                                </td>

                            </tr>
                        }
                    </tbody>
                </table>
            </div>
            <div class="modal-footer modal-footer-new1">

                Page &nbsp; <label type="label" id="Increatement_PageCount_FinishGoodsItem">1</label>
                &nbsp; of &nbsp;<label type="label" id="Total_PageCount_FinishGoodsItem">0</label> &nbsp; pages

                <span style="margin-left: 390px;">
                    <button type="button" id="0" onclick="F0_tblFinishGoodsItem_FIExp();">First</button>
