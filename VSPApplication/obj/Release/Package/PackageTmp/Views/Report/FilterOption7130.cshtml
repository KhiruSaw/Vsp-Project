                    <button type="button" id="3" onclick="F3_tblFinishGoodsItem_FIExp();">3</button> &nbsp;&nbsp;. . . . &nbsp;&nbsp;
                    <button type="button" id="+1" onclick="FPluse10_tblFinishGoodsItem_FIExp();">>></button>
                    <button type="button" id="99" onclick="F99_tblFinishGoodsItem_FIExp();">Last</button>
