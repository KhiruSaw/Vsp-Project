                                   new SelectListItem {Text="Name",Value="Name" },
                                  }, new { @id = "ddlCustomerID2PSPU", @class = "form-control" })
                        </div>
                        <div class="col-lg-3">
                            <input type="text" id="txtCustomerIDPSPU" class="form-control" />

                        </div>

                    </div>


                    <table class="table" id="tableciPS">
                        <br />
                        <thead>

                            <tr>
                                <th></th>
                                <th>
                                    Customer ID
                                </th>
                                <th>
                                    Name
                                </th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (BO_FilterOption ci in Model.CustomerID2)
                            {
                            <tr>
                                <td>
                                    <input type="radio" name="rdBtn1" id="rdBtn1" />
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => ci.CustomerID)
