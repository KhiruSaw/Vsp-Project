<div class="modal fade" id="myModalFinishGoodsID" role="dialog">
    <div class="modal-dialog modal-lg">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Finish Good ID</h4>
            </div>
            <div class="modal-body">



                <div class="col-md-12 searchfor">
                    <div class="col-lg-2 searchhead"><h4>Search For</h4></div>
                    <div class="col-lg-3">
                        @Html.DropDownList("FinishGoodsId9170", new List<SelectListItem> {
                                            new SelectListItem {Text="Start With",Value="Start With" },
                                             new SelectListItem {Text="End With",Value="End With" },
                                              new SelectListItem {Text="Contains This",Value="Contains This" },
                                             },  new { @id = "ddlFinishGoodsId_Beg", @class = "form-control" })
                    </div>
                    <div class="col-lg-3">
                        @Html.DropDownList("FinishGoodsId9170_2", new List<SelectListItem> {
                                   new SelectListItem {Text="Finished Good",Value="Finished Good" },
                                   new SelectListItem {Text="Description",Value="Description" },
                                   new SelectListItem {Text="Customer Part",Value="Customer Part" },
                                   new SelectListItem {Text="Customer ID",Value="Customer ID" },
                                   new SelectListItem {Text="Customer Name",Value="Customer Name" },
                                    },  new { @id = "ddlFinishGoodsId_End", @class = "form-control" })
                    </div>
                    <div class="col-lg-3">
                        <input type="text" id="txtFinishGoodsId9170" class="form-control" />
                        <input type="hidden" id="hdnFinishGoodsItem" />
                    </div>

                </div>


                <table class="table" id="tblFinishGoodsId9170">
                    <br />
                    <thead>
                        <tr>
                            <th></th>
                            <th>
                                Description
                            </th>
                            <th>
                                FinishedGood
                            </th>
                            <th>
                                CustomerPart
                            </th>
                            <th>
                                CustomerID
                            </th>
                            <th>
                                CustomerName
                            </th>

                        </tr>
                    </thead>
                    <tbody>
                        @foreach (BO_FilterOption teacher in Model.FinishGoodItem)
                        {
                            <tr>
                                <td>
                                    <input type="radio" name="rdBtnFinishGoodsId9170" id="rdBtnFinishGoodsId9170" />
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => teacher.Description)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => teacher.FinishedGood)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => teacher.CustomerPart)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => teacher.CustomerID)
                                </td>
