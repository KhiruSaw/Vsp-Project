    <script>
        /* First Time */
        $(function () {
          
            //$('#rangeval').hide();
            //$('#Textdiv5').show();
            //$('#datediv2').hide();
            $("#ddlDateShipped").change(function () {
              
                var text = $("#ddlDateShipped option:selected").text();
                if (text == "Between Range") {
                    $('#rangeval').hide();
                    $('#Textdiv5').hide();
                    $('#datediv1').show();
                    $('#datediv2').show();
                    //$("#DateShipped1").prop('enable', true);
                    $("#DateShipped1").datepicker();
                    $("#DateShipped2").datepicker();
                    $("#DateShipped1").datepicker().datepicker("setDate", new Date());
                    $("#DateShipped2").datepicker().datepicker("setDate", new Date());
                   
                }
                else if (text == "*All Records") {
                    $('#rangeval').show();
                    $('#Textdiv5').show();
                    $('#datediv1').hide();
                    $('#datediv2').hide();
                    $('#rangevaltext').val("*All Records");
                    $('#rangevaltext2').val("0");
                    //$('#DateShipped1').val("*All Records");
                    //$('#DateShipped2').val("0");

                }
                else {

                    $('#DateShipped2').datepicker("enable");
                    $('#datediv1').hide();
                    $('#rangeval').show();
                    $('#datediv2').show();
                    $('#Textdiv5').hide();
                    var ddlVal = $('#ddlDateShipped').val();
                    $('#rangevaltext').val(ddlVal);
                    $("#DateShipped2").datepicker().datepicker("setDate", new Date());

                }

            });
        
        });
    </script>
